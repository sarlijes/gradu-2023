@INPROCEEDINGS{4626426,
author={Duan, Feng and Zhang, Ye and Pongthanya, Nuttapol and Watanabe, Kei and Yokoi, Hiroshi and Arai, Tamio},
booktitle={2008 IEEE International Conference on Automation Science and Engineering},
title={Analyzing human skill through control trajectories and motion capture data},
year={2008},
volume={},
