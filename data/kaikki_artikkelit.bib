@INPROCEEDINGS{5070555,
author={Ying Zhang and Gang Huang and Nuyun Zhang and Hong Mei},
booktitle={2009 IEEE 31st International Conference on Software Engineering},
title={SmartTutor: Creating IDE-based interactive tutorials via editable replay},
year={2009},
volume={},
