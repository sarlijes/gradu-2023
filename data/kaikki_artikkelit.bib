year = {2017},
isbn = {9781538640418},
publisher = {IEEE Press},
url = {https://doi-org.ezproxy.jyu.fi/10.1109/CSI-SE.2017.6},
doi = {10.1109/CSI-SE.2017.6},
abstract = {This paper reports on a study that aimed to characterize how crowd workers experienced for the first time the use of TopCoder, a crowdsourcing platform for software development that implements a competitive model. We explored how they perceived collaboration in this setting, what challenges they faced to perform a single task, and reflect upon their suggestions to overcome the challenges their experienced. More specifically, we asked graduate students to select a development challenge task, work on it, and submit their contribution to the platform. Early analysis of the results: (1) reveal the potential benefits of software crowdsourcing from the crowd perspective, (2) discuss collaboration in a competitive model, and (3) highlight that the onboarding process for newcomers is seen as challenging. We discuss our findings in light of current literature.},
booktitle = {Proceedings of the 4th International Workshop on CrowdSourcing in Software Engineering},
pages = {2–8},
numpages = {7},
keywords = {software development competitive model, TopCoder, crowdsourcing, newcomers},
location = {Buenos Aires, Argentina},
series = {CSI-SE '17}
}

@inproceedings{10.1145/3463531.3463552,
author = {Liu, Shan and Md Hassan, Nurulhuda and Ning, Ke and Hu, Bisheng and Xu, Songgao},
title = {The Relationship between Physical Well-Being and Cognitive Outcomes in 2–7 Year Old Typically Developing Children: A Systematic Review},
year = {2021},
isbn = {9781450389662},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
url = {https://doi-org.ezproxy.jyu.fi/10.1145/3463531.3463552},
doi = {10.1145/3463531.3463552},
abstract = {Objectives: The aim of this review was to provide an overview of researches presenting evidence for a relationship between physical well-being and cognitive outcomes in 2–7 year old typically developing children. Design: A systematic review. Methods: Articles involved in this review were from Science Direct, EBSCO and WoS. In total, 21 articles were included in this review. Two reviewers assessed the methodological quality independently. Results: The evidence for or against the relationship between physical well-being and cognitive outcomes was not sufficient. However, there was weak-to-strong evidence for some relationships between underlying categories of physical well-being and cognitive outcomes, including complex motor skills and higher order cognitive skills. Conclusions: There were some relationships between motor and cognitive skills. It offered insight into possible interventions designed to complex motor skills which may be beneficial for both motor and higher order cognitive skills of preschool children.},
booktitle = {2021 7th International Conference on Education and Training Technologies},
pages = {132–138},
numpages = {7},
keywords = {systematic review, cognitive outcomes, physical fitness, preschool children, Motor skill},
location = {Macau, China},
series = {ICETT 2021}
}

@inproceedings{10.1145/3473714.3473827,
author = {Cheng, Yunli and Huang, Min and Lei, Yaohua and Tan, Xueqin},
