@INPROCEEDINGS{217587,
author={Urban, J.E. and Frye, R. and Miller, A. and Rao, A.G.},
booktitle={[1992] Proceedings. The Sixteenth Annual International Computer Software and Applications Conference},
title={Industrial strength software engineering training programs},
year={1992},
