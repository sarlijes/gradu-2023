abstract={User-centered design proponents advocate the proactive role of the user in the software development process. This study describes how novice software designers interpreted usability cessions and how these designers responded to their users’ reactions to their respective prototypes. Qualitative analysis of class projects, usability sessions, and interviews with three groups of graduate students took place. A follow-up session involved written protocols from another group of graduate students. Overall these usability sessions had minimal effect on these students’ prototypes and essentially did not yield any valuable information. In an effort to remedy this current situation, this study proposes recommendations on how to improve the training of educational software developers. © 2001 Taylor & Francis.},
document_type={Article},
source={Scopus},
}

@CONFERENCE{Dorneich20001140,
author={Dorneich, Michael C. and Jones, Patricia M.},
title={Systematic application of the apprenticeship learning pedagogy to computer tutorial design},
journal={Proceedings of the IEEE International Conference on Systems, Man and Cybernetics},
year={2000},
volume={2},
pages={1140-1145},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-0034513132&partnerID=40&md5=51da27dab98f65606c8049a4f846ad96},
abstract={This paper describes the Collaborative Apprenticeship Learning Object Toolkit (CALOT), a generalized architecture that forms the basis of a family of computer tutoring systems. The toolkit, based on previous work, utilizes the apprenticeship model of learning as the basis of the design of a computer tutorial system for the study of the operational procedures where theory and practice are both of importance. Systems built from the CALOT library teach complex operational procedures in a safe, simulated environment that allows students to explore relevant theoretical concepts in the context of their current activity. In addition, systems built upon the CALOT architecture can be utilized by multiple, non-co-located students in a synchronous manner - meaning that each student can run their own tutorial, and through a server every action taken by one student is immediately rebroadcast to the other students' tutorials. In this way a distributed set of students can collaborate on the tutorial in real-time. The development of this type of computer tutorial system is driven by the goal of linking theoretical knowledge with practical operational experience. Active, exploratory, apprentice-style learning is supported via modes of operation within the system. The student can flexibly choose to `observe the expert' perform and explain operational steps, or `act as an apprentice' and carry out the steps autonomously. The student can switch between these modes at their discretion, giving the student control of the level of intervention by the system. Furthermore, the student can explore and reflect on an `information space' of object designations, procedures, and related concepts. Two examples tutorials based on the CALOT architecture are the Collaborative Virtual Spectrometer (CVS) and the Virtual X-Ray Diffractometer (VXRD), which teach nuclear magnetic resonance spectroscopy and x-ray diffraction, respectively. Finally, BAUEN (Basic AUthoring ENvironment) is described. BAUEN is a graphical user interface system that is intended to help the software developer designer author a CALOT-based computer tutorial system with maximum efficiency, focusing the developer's efforts on domain content, and not on system implementation.},
publisher={IEEE, Piscataway, NJ, United States},
document_type={Conference Paper},
source={Scopus},
}

