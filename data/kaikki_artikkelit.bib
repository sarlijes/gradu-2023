@CONFERENCE{Gallerati2017,
author={Gallerati, P. and Bagnato, S. and Casciaro, D. and Conte, A. and Maisano, M.},
title={Use of serious gaming and virtual reality applications improves students' learning retention and reduces safety risks and costs associated with training activities},
journal={Offshore Mediterranean Conference and Exhibition 2017, OMC 2017},
