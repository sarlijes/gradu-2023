@INPROCEEDINGS{9951971,
author={Al-Ansari, Dhia and Abdulla, Raed and Selvaperumal, Sathish Kumar and Dhakshyani, R.},
booktitle={2022 3rd International Conference on Smart Electronics and Communication (ICOSEC)},
title={3D Simulation of MMA Punch Motion Tracking Using Wearable Inertia Measurement Unit Sensors in LabVIEW},
year={2022},
volume={},
number={},
pages={161-167},
abstract={Punching techniques are one of the challenges that martial arts trainers face while training the beginners. In order to teach the punching techniques in a proper way, trainers should constantly comment on the athlete’s performance. In some circumstances the best performing athlete will also perform incorrect punches. The main purpose of this study is to develop a 3D arm GUI to demonstrate the simplest karate punches, known as choku-zuki, performed by participants via wireless communication. The generated 3D image will then be analyzed by comparing the accuracy of the performed punch with an accurate reference and a stable punching pattern retrieved from karate experts. The proposed module is composed of MPU6050 IMU sensors to measure the rotation and angular velocity of wrist, forearm, and upper arm to determine the user’s punch. Then, the proposed system is tested on 10 female and male karate performers to obtain a pattern and 10 female and male newcomers to obtain another pattern. Later, both the data will be used to evaluate the system functionality and illustrate the accuracy of performed punch with respect to pattern. The results showed that the control group could match their punch with the pattern after three attempts. It was also observed that the participants were unable to maintain their performance, as some achieved good results in terms of pattern, but lost accuracy in their second and third attempts. The major issue with newcomers is related to their hand rotation and varying angular velocity.},
keywords={Wrist;Three-dimensional displays;Punching;Measurement uncertainty;Software;Sensor systems;Sensors},
doi={10.1109/ICOSEC54921.2022.9951971},
ISSN={},
month={Oct},}
@ARTICLE{9127441,
author={Alves, Fábio Junior and De Carvalho, Emerson Assis and Aguilar, Juliana and De Brito, Lucelmo Lacerda and Bastos, Guilherme Sousa},
journal={IEEE Access},
title={Applied Behavior Analysis for the Treatment of Autism: A Systematic Review of Assistive Technologies},
year={2020},
volume={8},
number={},
pages={118664-118672},
abstract={Autism Spectrum Disorder (ASD) is a neurodevelopmental disorder that may lead to significant impairment in social communication, repetitive patterns of behavior, and possible fixed and restricted interests. Applied Behavior Analysis (ABA) is a well-supported and evidence-based treatment for the delays attributed to ASD. Assistive technologies, such as gamification, software apps, computer-based training (Web), and robotics; provide a standardized method of implementing ABA techniques. This review provides a synthesis of the main characteristics of these technologies. The assessed proposals focused on technologies such as Distributed Systems, Image Processing, Gamification, and Robotics. The primary objectives of these tools sought to improve social behavior, attention, communication, and reading skills. Some common limitations found in the literature was a failure to accurately define their target audience, and a failure to comply with the dimensions of ABA as defined by Baer, Wolf, and Risley in 1968.},
keywords={Assistive technology;Autism;Systematics;Databases;Robots;Proposals;Variable speed drives;Autism spectrum disorder;applied behavior analysis;assistive technologies},
doi={10.1109/ACCESS.2020.3005296},
ISSN={2169-3536},
month={},}
@INPROCEEDINGS{9491624,
author={Wang, XiaoYu and Zhou, Hao and Freris, Nikolaos M. and Zhou, Wangqiu and Guo, Xing and Li, Xiang-Yang},
booktitle={2021 18th Annual IEEE International Conference on Sensing, Communication, and Networking (SECON)},
title={CALM: Contactless Accurate Load Monitoring via Modality Distillation},
year={2021},
volume={},
number={},
pages={1-9},
abstract={The rapid proliferation of Smart Grids calls for a more in-depth understanding of user energy consumption behaviors, based on large data volumes collected by various sources of sensors such as voltmeter and ammeter. Non-Intrusive Load Monitoring (NILM) is a single sensor solution, which can effectively disaggregate individual appliance states from measurements only at the interface to the power source, albeit at the cost of requiring circuit modifications thus introducing suspension of services and potential safety hazards. To overcome the undesirable attribute of NILM and achieve a safe yet highly accurate solution, we devise a contactless sensing system based on inductive current measurements that can conduct load disaggregation without tampering with the power system. Despite using single modality, i.e., the inductive current, our scheme attains state-of-the-art accuracy in existing multi-modality datasets by leveraging modality distillation technique to handle arbitrary input structure. Our main contributions enlist: (1) devising and deploying the first, to the best of our knowledge, purely contactless non-intrusive load disaggregation system; (2) the design of an oracle-apprentice network structure to leverage multi-modality input for training, while operating with single modality; (3) a high estimation accuracy of 95.44% and 96.21%, respectively, is attested on two public datasets, which proves the efficiency of our method.},
keywords={Training;Energy consumption;Current measurement;Voltmeters;Sensor phenomena and characterization;Robustness;Smart grids},
doi={10.1109/SECON52354.2021.9491624},
ISSN={2155-5494},
month={July},}
@ARTICLE{6448989,
author={Rosenbaum, Stephanie and Walters, R. Dennis},
journal={IEEE Transactions on Professional Communication},
title={Audience diversity: A major challenge in computer documentation},
year={1986},
volume={PC-29},
