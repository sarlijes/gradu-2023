@INPROCEEDINGS{9220257,
author={Mejía Gamarra, José Enrique and Alonso Salazar Cubas, Martín and Sosa Silupú, Junior David and Enrique Córdova Chirinos, Carlos},
booktitle={2020 IEEE XXVII International Conference on Electronics, Electrical Engineering and Computing (INTERCON)},
title={Prototype for Peruvian Sign Language translation based on an artificial neural network approach},
year={2020},
volume={},
