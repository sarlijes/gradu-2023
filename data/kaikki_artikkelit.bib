@INPROCEEDINGS{9543080,
author={Souza, Frederico D. and Barbosa, Camila and Gonçalves, João and Furtado, Victor and Amaro, Amanda and Pena, Felipe and Reis, Ranielly C. and Floriano, Adrisson C. and De Oliveira Júnior, Reginaldo},
booktitle={2021 IEEE PES Innovative Smart Grid Technologies Conference - Latin America (ISGT Latin America)},
title={Multi-Label Classification of Voice Calls from Power Plant Operation Centers},
year={2021},
volume={},
number={},
pages={1-5},
abstract={This paper presents a machine learning-based pipeline to classify, in six labels, voice calls from power plant operation centers from ENGIE Brasil Energia (private power producer). The pipeline consists of a customized speech-to-text model from Amazon Web Services (AWS) followed by a multi-label text classification model. Our experiments showed how to leverage the performance of Amazon Transcribe with a custom vocabulary, as well as the predictive performance for different tree-based machine learning models. The work aims to facilitate the audit of internal actions and increase operational efficiency from the post-operation activities from ENGIE Brasil Energia (EBE). We achieved great predictive results, high accuracy, and Fl-score for all six labels.},
keywords={Training;Solid modeling;Vocabulary;Web services;Text recognition;Pipelines;Text categorization;machine learning;supervised learning;multi-label classification;speech recognition;speech-to-text;natural language processing;text classification;post-operation},
doi={10.1109/ISGTLatinAmerica52371.2021.9543080},
